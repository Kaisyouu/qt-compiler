main {
    int a[10];
    int i;
    i = 0;
    while (i < 10) {
        a[i] = i;
        write a[i];
        i = i + 1;
    }
    i = 0;
    while (i < 10) {
        write a[i] + 1;
        i = a[i] + a[i] + 1; // multiple rvalues are supported, but no array lvalue in this case
        write i;
    }
}